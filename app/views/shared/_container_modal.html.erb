<div class="modal-dialog">
<div class="modal-content">
  <div class="modal-header">
   <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;          </button>
    <h3 class="modal-title" id="myModalLabel">Container</h3>
  </div>
  <div class="modal-body">
  <div class="row">
	  <div class="col-md-3">
	    <p class="text-left">Container ID</p>
	  </div>   
	  <div class="col-md-9">
	    <p class="text-muted"><%= @sample.container.id %></p>
	  </div>
	</div>
	
    <div class="row">
	  <div class="col-md-3">
	    <p class="text-left">Container Type</p>
	  </div>   
	  <div class="col-md-9">
	    <p class="text-muted"><%= @sample.container.type %></p>
	  </div>
	</div>
	
	<div class="row">
	  <div class="col-md-3">
	    <p class="text-left">Description</p>
	  </div>   
	  <div class="col-md-9">
	    <p class="text-muted"><%= @sample.facility.description %></p>
	  </div>
	</div>
	
    <div class="row">
	  <div class="col-md-3">
	    <p class="text-left">Contact</p>
	  </div>   
	  <div class="col-md-9">
	    <p class="text-muted"><%= @sample.facility.contact.firstname %> <%= @sample.facility.contact.surname %></p>
	  </div>
	</div>
	
  </div>
</div>
</div>